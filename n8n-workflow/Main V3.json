      "id": "c377ac1c-3314-4d52-9652-85c5cb13679f",
      "name": "Load Memory"
    },
    {
      "parameters": {
        "jsCode": "// üß© Update Memory ‚Äì schreibt Antwort + Frage pro conversation_id in globalen Speicher\n\nfunction norm(x){ return String(x ?? '').trim(); }\nfunction valid(x){ return /^[A-Za-z0-9._:-]{8,200}$/.test(x); }\n\nconst staticData = $getWorkflowStaticData('global');\nstaticData._memory = staticData._memory || {};\n\n// üîπ conversation_id aus JSON / Body / Header extrahieren\nconst body = $json.body || {};\nconst hdr = $json.headers || {};\nconst query = $json.query || {};\n\nlet cid = norm(\n  $json.conversation_id ??\n  body.conversation_id ??\n  hdr['x-conversation-id'] ??\n  hdr['X-Conversation-Id'] ??\n  query.conversation_id ?? ''\n);\n\nif (!valid(cid)) {\n  throw new Error('‚ùå conversation_id fehlt oder ung√ºltig ‚Äì kann Memory nicht updaten');\n}\n\n// üîπ Vorhandenes Memory holen oder leeren Verlauf erstellen\nconst mem = staticData._memory[cid] || { history: [], summary: '', updated_at: null };\n\n// üîπ Frage & Antwort auslesen\nconst userPrompt = $json._original_prompt ?? $json.prompt ?? $json.body?.prompt ?? '';\nconst answer = $json.answer ?? $json.text ?? $json.result ?? '';\nconst model = $json.model ?? $json.provider ?? 'unknown';\n\n// üîπ Eintr√§ge anh√§ngen\nmem.history.push(\n  { role: 'user', content: userPrompt },\n  { role: 'assistant', content: answer, model }\n);\n\n// üîπ Zeit & optionale Summary √ºbernehmen\nmem.updated_at = new Date().toISOString();\nif ($json.summary) mem.summary = $json.summary;\n\n// üîπ Limitierung optional\nconst maxTurns = 20;\nif (mem.history.length > maxTurns * 2) {\n  mem.history = mem.history.slice(-maxTurns * 2);\n}\n\n// üîπ Abspeichern\nstaticData._memory[cid] = mem;\n\n// üîπ Ausgabe an n√§chste Node\nreturn [\n  {\n    json: {\n      ...$json,\n      conversation_id: cid,\n      memory: mem,\n      updated_at: mem.updated_at,\n    },\n  },\n];\n"
      },
